	keywords = {Health inequalities, Capitalism, Friedrich Engels, Social murder},
	pages = {114377},
	file = {ScienceDirect Snapshot:/Users/madeline/Zotero/storage/6N8JX364/S0277953621007097.html:text/html},
}

