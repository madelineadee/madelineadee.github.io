	file = {Full Text PDF:/Users/madeline/Zotero/storage/EB7YTWL2/Westreich and Greenland - 2013 - The Table 2 Fallacy Presenting and Interpreting C.pdf:application/pdf;Snapshot:/Users/madeline/Zotero/storage/NZGZZ8M8/147738.html:text/html},
}

