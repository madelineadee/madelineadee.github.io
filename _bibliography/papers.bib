@article{aisyah_assessing_2018,
	title = {Assessing hepatitis {C} spontaneous clearance and understanding associated factors--{A} systematic review and meta-analysis.},
	volume = {25},
	issn = {13520504},
	url = {https://search.ebscohost.com/login.aspx?direct=true&db=a9h&AN=129961518&site=ehost-live},
	abstract = {New advances in the treatment of hepatitis C provide high levels of sustained viral response but their expense limits availability in publicly funded health systems. The aim of this review was to estimate the proportion of patients who will spontaneously clear HCV, to identify factors that are associated with clearance and to support better targeting of directly acting antivirals. We searched Ovid EMBASE, Ovid MEDLINE and PubMed from 1 January 1994 to 30 June 2015 for studies reporting hepatitis C spontaneous clearance and/or demographic, clinical and behavioural factors associated with clearance. We undertook meta-analyses to estimate the odds of clearance for each predictor. Forty-three studies met the inclusion criteria, representing 20 110 individuals, and 6 of these studies included sufficient data to estimate spontaneous clearance. The proportion achieving clearance within 3, 6, 12 and 24 months following infection were, respectively, 19.8\% (95\% CI: 2.6\%-47.5\%), 27.9\% (95\% CI: 17.2\%-41.8\%), 36.1\% (95\% CI: 23.5\%-50.9\%) and 37.1\% (95\% CI: 23.7\%-52.8\%). Individuals who had not spontaneously cleared by 12 months were unlikely to do so. The likelihood of spontaneous clearance was lower in males and individuals with HIV co-infection, the absence of HBV co-infection, asymptomatic infection, black or nonindigenous race, nongenotype 1 infection, older age and alcohol or drug problems. This study suggests that patients continue to spontaneously clear HCV for at least 12 months following initial infection. However, injecting drug users are comparatively less likely to achieve clearance; thus, they should be considered a priority for early treatment given the continuing risks that these individuals pose for onwards transmission. [ABSTRACT FROM AUTHOR]},
	number = {6},
	journal = {Journal of Viral Hepatitis},
	author = {Aisyah, D. N. and Shallcross, L. and Hully, A. J. and O'Brien, A. and Hayward, A.},
	month = jun,
	year = {2018},
	note = {Publisher: Wiley-Blackwell},
	keywords = {ANTIVIRAL agents, behaviour, clinical, demographic factor, HEPATITIS C treatment, hepatitis C virus, HEPATITIS C virus, META-analysis, predictors, SYSTEMATIC reviews},
	pages = {680--698},
}

